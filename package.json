    "@commitlint/config-conventional": "19.8.0",
    "@vitest/coverage-v8": "3.0.9",
    "husky": "9.1.7",
    "tsup": "8.4.0",
    "turbo": "2.4.4",
    "typescript": "5.8.2",
    "ultracite": "4.2.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.9"
  }
}
