{
  "name": "restkit",
  "private": true,
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "prepare": "husky",
    "lint": "ultracite lint",
    "format": "ultracite format",
    "typecheck": "turbo typecheck",
